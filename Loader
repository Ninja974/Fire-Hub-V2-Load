local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local key = "TU-KEY"
local placeid = game.PlaceId
local url = "http://51.75.118.75:8000/validate?key="..key.."&hwid="..hwid.."&placeid="..placeid

local success, response = pcall(function()
    return game:HttpGet(url)
end)

if success then
    loadstring(response)()
else
    warn("‚ùå Acceso denegado.")
end
