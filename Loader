local HttpService = game:GetService("HttpService")
local PlaceId = game.PlaceId
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()

local KEY = "" -- 


local Library = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local Window = Library:CreateWindow({
    Title = "Fire Hub üî•",
    SubTitle = "Key Required",
    TabWidth = 120,
    Size = UDim2.fromOffset(370, 230),
    Acrylic = true,
    Theme = "Dark",
    Center = true,
    IsResizable = false
})
local Tabs = { Main = Window:AddTab({ Title = "Key", Icon = "üîë" }) }

Tabs.Main:AddInput("KeyBox", {
    Title = "Enter Key",
    Placeholder = "Paste your key...",
    Callback = function(value)
        KEY = value
    end
})

Tabs.Main:AddButton({
    Title = "‚úÖ Submit Key",
    Callback = function()
        local url = string.format(
            "http://51.75.118.75:20256/map_script?key=%s&hwid=%s&placeid=%d",
            KEY,
            hwid,
            PlaceId
        )

        local success, response = pcall(function()
            return game:HttpGet(url)
        end)

        if not success then
            return Library:Notify({ Title = "‚ùå Error", Content = "Can't reach server.", Duration = 4 })
        end

        local data = HttpService:JSONDecode(response)

        if not data.valid then
            return Library:Notify({ Title = "‚ùå Invalid", Content = data.reason or "Access denied", Duration = 4 })
        end

        Library:Notify({ Title = "‚úî Access Granted", Content = "Loading Fire Hub...", Duration = 3 })
        task.wait(1)
        Window:Destroy()


        local success2, err = pcall(function()
            loadstring(data.script)()
        end)

        if not success2 then
            warn("‚ùå Script error:", err)
        end
    end
})
